#include"Robot.h"

#include "DLL/DLL_Interface.h"
DNode_t* Frontier = NULL ;

State_t Suorce ={    .Row = 6 , .Col = 8 };

State_t Distination={.Row = 5 , .Col = 5 };

extern State_t Current ;

extern AvailableActions_t MyAvailableAcions ;

int main(void)
{

	INITIAL_STATE( &Suorce );
	ACTIONS(&MyAvailableAcions);

	if(MyAvailableAcions.down == true  )
	{
		printf("Down\n");

	}
	if(MyAvailableAcions.up == true  )
	{
		printf("Up\n");

	}
	if(MyAvailableAcions.right == true  )
	{
		printf("Right\n");
	}

	if(MyAvailableAcions.left == true  )
	{
		printf("Left\n");
	}


   RESULT( &Current , &MyAvailableAcions);


}//End of main.
